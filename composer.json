{
  "require": {
    "craftcms/cms": "^4.4",
    "markhuot/craft-pest-core": "^2.0.0-rc5",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "pestphp/pest-plugin": true
    },
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true
  }
}
